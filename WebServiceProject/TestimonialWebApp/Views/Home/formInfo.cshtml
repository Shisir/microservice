<form method="post" action="/deptVerify/" class="bg-white rounded pb_form_v1">
    <div class="form-group">
        <label for="name">Name</label>
        <input type="text" readonly="readonly" id="name" name="name" class="form-control pb_height-30 reverse" value='@TempData["name"]' >     
    </div>
    <div class="form-group">
        <label for="regNum">Registration Number</label>
        <input type="text" readonly="readonly" id="regNum" name="regNum" class="form-control pb_height-30 reverse" value='@TempData["regNum"]' >     
    </div>
    
    <div class="form-group">
        <label for="fatherName">Father Name</label>
        <input type="text" readonly="readonly" id="fatherName" name="fatherName" class="form-control pb_height-30 reverse" value='@TempData["fatherName"]' >     
    </div>
    
    <div class="form-group">
        <label for="motherName">Mother Name</label>
        <input type="text" readonly="readonly" id="motherName" name="motherName" class="form-control pb_height-30 reverse" value='@TempData["motherName"]' >     
    </div>
    
    <div class="form-group">
        <label for="hall">Hall</label>
        <input type="text" readonly="readonly" id="hall" name="hall" class="form-control pb_height-30 reverse" value='@TempData["hall"]' >     
    </div>
    
    <div class="form-group">
        <label for="currentSemester">Current Semester</label>
        <input type="text" readonly="readonly" id="currentSemester" name="currentSemester" class="form-control pb_height-30 reverse" value='@TempData["currentSemester"]' >     
    </div>
    
    <div class="form-group">
        <small class="error">
            @{  
                var message = "";
                if(TempData["dept_error"]!=null){
                    message = TempData["dept_error"].ToString();
                }
            }
            @message
        </small> 
        <input type="submit" name="submit" class="btn btn-primary btn-lg btn-block pb_btn-pill  btn-shadow-blue" value="Submit For Dept. Verification">
        <input type="submit" name="submit" class="btn btn-primary btn-lg btn-block pb_btn-pill  btn-shadow-blue" value="Go Back">
    </div>
   
</form>

